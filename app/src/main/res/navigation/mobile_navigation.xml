<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_auto">

    <fragment
        android:id="@+id/navigation_auto"
        android:name="com.example.a2023_scouting_v2.ui.auto.AutoFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_auto" />

    <fragment
        android:id="@+id/navigation_driver"
        android:name="com.example.a2023_scouting_v2.ui.driver.DriverFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_driver" />

    <fragment
        android:id="@+id/navigation_endgame"
        android:name="com.example.a2023_scouting_v2.ui.endgame.EndgameFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_endgame" />

    <fragment
        android:id="@+id/navigation_data"
        android:name="com.example.a2023_scouting_v2.ui.data.DataFragment"
        android:label="data"
        tools:layout="@layout/fragment_data" />
</navigation>