<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#323435"
    tools:context=".ui.auto.AutoFragment">

    <TextView
        android:id="@+id/textAutoTop"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:textAlignment="center"
        android:textColor="@color/design_default_color_background"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/topMinusA"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="17dp"
        android:layout_marginEnd="10dp"
        android:backgroundTint="@color/white"
        android:text="-"
        android:textColor="@color/design_default_color_background"
        android:textSize="60sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/topButtonsText2"
        app:layout_constraintEnd_toStartOf="@+id/topScoreTA"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/topButtonsText" />

    <Button
        android:id="@+id/topPlusA"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="@color/white"
        android:text="+"
        android:textColor="@color/design_default_color_background"
        android:textSize="60sp"
        app:layout_constraintBottom_toTopOf="@+id/topButtonsText2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/topScoreTA"
        app:layout_constraintTop_toTopOf="@+id/topMinusA"
        tools:text="+" />

    <Button
        android:id="@+id/midMinusA"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:backgroundTint="@color/white"
        android:text="-"
        android:textColor="@color/design_default_color_background"
        android:textSize="60sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/topButtonsText3"
        app:layout_constraintEnd_toStartOf="@+id/midScoreTA"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/topButtonsText2" />

    <Button
        android:id="@+id/lowPlusA"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="@color/white"
        android:text="+"
        android:textColor="@color/design_default_color_background"
        android:textSize="60sp"
        app:layout_constraintBottom_toBottomOf="@+id/lowMinusA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/lowScoreTA"
        app:layout_constraintTop_toBottomOf="@+id/topButtonsText3" />

    <Button
        android:id="@+id/lowMinusA"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="@color/white"
        android:text="-"
        android:textColor="@color/design_default_color_background"
        android:textSize="60sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/lowScoreTA"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/topButtonsText3" />

    <TextView
        android:id="@+id/topScoreTA"
        android:layout_width="75dp"
        android:layout_height="73dp"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@color/design_default_color_background"
        android:textSize="60sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/topMinusA"
        app:layout_constraintEnd_toStartOf="@+id/topPlusA"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/topMinusA"
        app:layout_constraintTop_toTopOf="@+id/topMinusA" />

    <TextView
        android:id="@+id/midScoreTA"
        android:layout_width="75dp"
        android:layout_height="73dp"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@color/design_default_color_background"
        android:textSize="60sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/midMinusA"
        app:layout_constraintEnd_toStartOf="@+id/midPlusA"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/midMinusA"
        app:layout_constraintTop_toBottomOf="@+id/topButtonsText2" />

    <TextView
        android:id="@+id/lowScoreTA"
        android:layout_width="75dp"
        android:layout_height="73dp"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@color/design_default_color_background"
        android:textSize="60sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/lowMinusA"
        app:layout_constraintEnd_toStartOf="@+id/lowPlusA"
        app:layout_constraintStart_toEndOf="@+id/lowMinusA"
        app:layout_constraintTop_toBottomOf="@+id/topButtonsText3" />

    <TextView
        android:id="@+id/topButtonsText3"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:text="Low Score"
        android:textColor="@color/design_default_color_background"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/lowMinusA"
        app:layout_constraintEnd_toEndOf="@+id/midMinusA"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/midMinusA" />

    <TextView
        android:id="@+id/topButtonsText"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:text="High Score"
        android:textColor="@color/design_default_color_background"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/topMinusA"
        app:layout_constraintEnd_toEndOf="@+id/topMinusA"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/community"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/topButtonsText2"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:text="Middle Score"
        android:textColor="@color/design_default_color_background"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/midMinusA"
        app:layout_constraintEnd_toEndOf="@+id/topMinusA"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/topMinusA" />

    <Button
        android:id="@+id/midPlusA"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="@color/white"
        android:text="+"
        android:textColor="@color/design_default_color_background"
        android:textSize="60sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/midMinusA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/midScoreTA"
        app:layout_constraintTop_toTopOf="@+id/midMinusA" />

    <EditText
        android:id="@+id/scoutNameI"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:hint="Scout Name"
        android:inputType="textPersonName"
        android:textColor="@color/design_default_color_surface"
        android:textColorHint="#919191"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textAutoTop" />

    <EditText
        android:id="@+id/matchNumI"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:hint="Match #"
        android:inputType="number"
        android:textColor="@color/design_default_color_surface"
        android:textColorHint="#919191"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scoutNameI" />

    <Button
        android:id="@+id/submitInfo"
        android:layout_width="150dp"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="@color/white"
        android:text="Submit"
        android:textColor="@color/design_default_color_background"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/engaged"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/matchNumI" />

    <Button
        android:id="@+id/docked"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="75dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="@color/white"
        android:text="Docked"
        android:textColor="@color/design_default_color_background"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/community"
        app:layout_constraintEnd_toStartOf="@+id/engaged"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/matchNumI" />

    <Button
        android:id="@+id/engaged"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="@color/white"
        android:text="Engaged"
        android:textColor="@color/design_default_color_background"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/community"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/docked"
        app:layout_constraintTop_toTopOf="@+id/docked" />

    <Button
        android:id="@+id/community"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="@color/white"
        android:text="Mobility"
        android:textColor="@color/design_default_color_background"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/topMinusA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/docked" />

    <Spinner
        android:id="@+id/teamNumI"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/custom_dropdown"
        android:backgroundTint="@color/white"
        android:backgroundTintMode="add"
        android:dropDownSelector="@color/design_default_color_background"
        app:layout_constraintBottom_toTopOf="@+id/docked"
        app:layout_constraintEnd_toStartOf="@+id/submitInfo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/matchNumI"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>